#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules/node-pre-gyp/bin/node-pre-gyp/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules/node-pre-gyp/bin/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules/node-pre-gyp/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules:/home/workspace/nodejs/coding-assignments/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/node-pre-gyp/bin/node-pre-gyp/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/node-pre-gyp/bin/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/node-pre-gyp/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-pre-gyp/bin/node-pre-gyp" "$@"
else 
  exec node  "$basedir/../node-pre-gyp/bin/node-pre-gyp" "$@"
fi

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/semver/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/.pnpm/semver@5.7.1/node_modules/semver/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/.pnpm/semver@5.7.1/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules:/home/workspace/nodejs/coding-assignments/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/semver/bin/semver/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/semver/bin/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/semver/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver" "$@"
else 
  exec node  "$basedir/../semver/bin/semver" "$@"
fi

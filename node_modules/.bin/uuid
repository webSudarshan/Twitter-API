#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/bin/uuid/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/bin/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/.pnpm/uuid@3.4.0/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules:/home/workspace/nodejs/coding-assignments/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/uuid/bin/uuid/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/uuid/bin/node_modules:/home/workspace/nodejs/coding-assignments/coding-assignment-2/node_modules/uuid/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../uuid/bin/uuid" "$@"
else 
  exec node  "$basedir/../uuid/bin/uuid" "$@"
fi
